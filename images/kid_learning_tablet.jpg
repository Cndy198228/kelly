<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <defs>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="0" dy="4" stdDeviation="8" flood-color="rgba(0,0,0,0.2)"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="#f8f9fa"/>
  
  <!-- Wooden shelf -->
  <rect x="30" y="180" width="220" height="380" rx="10" fill="#deb887"/>
  
  <!-- Books and objects on shelf -->
  <rect x="50" y="200" width="160" height="10" rx="2" fill="#8b4513"/>
  <rect x="60" y="320" width="140" height="10" rx="2" fill="#8b4513"/>
  
  <!-- Books -->
  <rect x="70" y="220" width="30" height="90" rx="2" fill="#ff6b6b"/>
  <rect x="110" y="230" width="30" height="80" rx="2" fill="#4ecdc4"/>
  <rect x="150" y="210" width="30" height="100" rx="2" fill="#45b7d1"/>
  
  <rect x="70" y="340" width="25" height="70" rx="2" fill="#f7dc6f"/>
  <rect x="105" y="350" width="35" height="60" rx="2" fill="#bb8fce"/>
  <rect x="150" y="345" width="25" height="65" rx="2" fill="#85c1e9"/>
  
  <!-- Plant -->
  <ellipse cx="190" cy="210" rx="40" ry="50" fill="#82e0aa"/>
  <rect x="185" y="260" width="10" height="40" fill="#7d8c4b"/>
  
  <!-- Table -->
  <rect x="30" y="480" width="740" height="120" rx="10" fill="#f5deb3" filter="url(#shadow)"/>
  <rect x="250" y="530" width="300" height="70" rx="10" fill="#f0e68c"/>
  
  <!-- Child -->
  <ellipse cx="350" cy="350" rx="80" ry="130" fill="#ffcccb"/>
  
  <!-- Hair -->
  <path d="M270,320 Q350,220 430,320 Q390,290 350,280 Q310,290 270,320 Z" fill="#8b5a2b"/>
  
  <!-- Face -->
  <circle cx="350" cy="350" r="60" fill="#ffcccb"/>
  
  <!-- Closed eyes with smile -->
  <path d="M320,340 Q330,350 340,340" stroke="#000" stroke-width="2" fill="none"/>
  <path d="M360,340 Q370,350 380,340" stroke="#000" stroke-width="2" fill="none"/>
  <path d="M330,370 Q350,385 370,370" stroke="#000" stroke-width="2" fill="none"/>
  
  <!-- Arm and hand -->
  <ellipse cx="310" cy="420" rx="40" ry="15" fill="#ffcccb"/>
  <circle cx="270" cy="410" r="25" fill="#ffcccb"/>
  
  <!-- Hand with raised fingers -->
  <rect x="270" y="385" width="8" height="25" rx="4" fill="#ffcccb"/>
  <rect x="285" y="390" width="8" height="20" rx="4" fill="#ffcccb"/>
  <rect x="260" y="390" width="8" height="20" rx="4" fill="#ffcccb"/>
  <rect x="295" y="400" width="8" height="10" rx="4" fill="#ffcccb"/>
  
  <!-- Body -->
  <rect x="320" y="410" width="60" height="100" rx="10" fill="#b22222"/>
  <rect x="320" y="410" width="60" height="20" rx="5" fill="#ffffff"/>
  
  <!-- Tablet -->
  <rect x="380" y="470" width="300" height="200" rx="10" fill="#ffffff" filter="url(#shadow)"/>
  <rect x="400" y="490" width="260" height="160" rx="5" fill="#f0f0f0"/>
  
  <!-- Book on table -->
  <rect x="150" y="490" width="100" height="20" rx="5" fill="#87ceeb"/>
  
  <!-- Clothing rack -->
  <line x1="550" y1="100" x2="750" y2="100" stroke="#a9a9a9" stroke-width="3"/>
  <line x1="600" y1="100" x2="600" y2="350" stroke="#a9a9a9" stroke-width="3"/>
  <line x1="700" y1="100" x2="700" y2="350" stroke="#a9a9a9" stroke-width="3"/>
  
  <!-- Clothes -->
  <path d="M610,100 Q615,160 610,220 L600,220 Q595,160 600,100 Z" fill="#4682b4" opacity="0.8"/>
  <path d="M690,100 Q685,170 690,240 L700,240 Q705,170 700,100 Z" fill="#d2b48c" opacity="0.8"/>
  <path d="M730,100 Q725,150 730,200 L740,200 Q745,150 740,100 Z" fill="#6495ed" opacity="0.8"/>
</svg>